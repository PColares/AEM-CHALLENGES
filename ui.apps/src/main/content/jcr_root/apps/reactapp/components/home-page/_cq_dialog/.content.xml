<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0"
          xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="nt:unstructured"
          jcr:title="Home Page"
          sling:resourceType="cq/gui/components/authoring/dialog">
   <content jcr:primaryType="nt:unstructured"
            sling:resourceType="granite/ui/components/coral/foundation/container">
      <layout jcr:primaryType="nt:unstructured"
              margin="{Boolean}false"
              sling:resourceType="granite/ui/components/foundation/layouts/fixedcolumns"/>
      <items jcr:primaryType="nt:unstructured">
         <column jcr:primaryType="nt:unstructured"
                 sling:resourceType="granite/ui/components/coral/foundation/container">
            <items jcr:primaryType="nt:unstructured">
               <logoImageResType jcr:primaryType="nt:unstructured"
                                 name="./logoImage/sling:resourceType"
                                 sling:resourceType="granite/ui/components/coral/foundation/form/hidden"
                                 value="core/wcm/components/image/v2/image"/>
               <logoImage allowUpload="{Boolean}false"
                          autoStart="{Boolean}false"
                          class="cq-droptarget"
                          cq-msm-lockable="./logoImage/file"
                          fieldLabel="logo Image"
                          fileNameParameter="./logoImage/fileName"
                          fileReferenceParameter="./logoImage/fileReference"
                          jcr:primaryType="nt:unstructured"
                          mimeTypes="[image/gif,image/jpeg,image/png,image/webp,image/tiff,image/svg+xml]"
                          multiple="{Boolean}false"
                          name="./logoImage/file"
                          sling:resourceType="cq/gui/components/authoring/dialog/fileupload"
                          title="Drag to select image"
                          uploadUrl="${suffix.path}"
                          useHTML5="{Boolean}true"/>
               <titleText1 cq-msm-lockable="./titleText1"
                           fieldLabel="title Text 1"
                           jcr:primaryType="nt:unstructured"
                           name="./titleText1"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <titleText2 cq-msm-lockable="./titleText2"
                           fieldLabel="title Text 2"
                           jcr:primaryType="nt:unstructured"
                           name="./titleText2"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <titleText3 cq-msm-lockable="./titleText3"
                           fieldLabel="title Text 3"
                           jcr:primaryType="nt:unstructured"
                           name="./titleText3"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <titleText4 cq-msm-lockable="./titleText4"
                           fieldLabel="title Text 4"
                           jcr:primaryType="nt:unstructured"
                           name="./titleText4"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <paragraphText1 cq-msm-lockable="./paragraphText1"
                               fieldLabel="paragraph Text 1"
                               jcr:primaryType="nt:unstructured"
                               name="./paragraphText1"
                               sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <paragraphText2 cq-msm-lockable="./paragraphText2"
                               fieldLabel="paragraph Text 2"
                               jcr:primaryType="nt:unstructured"
                               name="./paragraphText2"
                               sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <paragraphText3 cq-msm-lockable="./paragraphText3"
                               fieldLabel="paragraph Text 3"
                               jcr:primaryType="nt:unstructured"
                               name="./paragraphText3"
                               sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <paragraphText4 cq-msm-lockable="./paragraphText4"
                               fieldLabel="paragraph Text 4"
                               jcr:primaryType="nt:unstructured"
                               name="./paragraphText4"
                               sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <footerText cq-msm-lockable="./footerText"
                           fieldLabel="footer Text"
                           jcr:primaryType="nt:unstructured"
                           name="./footerText"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <refreshText cq-msm-lockable="./refreshText"
                            fieldLabel="refresh Text"
                            jcr:primaryType="nt:unstructured"
                            name="./refreshText"
                            sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <navigateButtonText cq-msm-lockable="./navigateButtonText"
                                   fieldLabel="navigate Button Text"
                                   jcr:primaryType="nt:unstructured"
                                   name="./navigateButtonText"
                                   sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
               <logoutText cq-msm-lockable="./logoutText"
                           fieldLabel="logout Text"
                           jcr:primaryType="nt:unstructured"
                           name="./logoutText"
                           sling:resourceType="granite/ui/components/coral/foundation/form/textfield"/>
            </items>
         </column>
      </items>
   </content>
</jcr:root>
